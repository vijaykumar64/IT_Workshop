//subimages

\documentclass{article}
\usepackage{graphicx}
\usepackage{subcaption}
\begin{document}
	\vspace{-15cm}
	\begin{figure}
	

	\begin{subfigure} {0.6\textwidth}
	
	\centering
	\includegraphics[width=0.8\textwidth]{img.jpg}
	\caption{image1}
    \end{subfigure}
    \hfill
    \begin{subfigure}{0.6\textwidth}
    
    \centering
    \includegraphics[width=0.8\textwidth]{img1.jpg}
    \caption{image1}
    \end{subfigure}


	\end{figure}
\end{document}




